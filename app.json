{
  "name": "did-it",
  "slug": "did-it",
  "icon": "./assets/logo-white-background.png",
  "userInterfaceStyle": "automatic",
  "scheme": "did-it",
  "newArchEnabled": true,
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "backgroundColor": "#FFFFFF",
        "image": "./assets/logo-blue-background.png",
        "dark": {
          "image": "./assets/logo-blue-background.png",
          "backgroundColor": "#FFFFFF"
        },
        "imageWidth": 200
      }
    ],
    ["expo-font", {
      "fonts": [
        {
          "Roboto": "./assets/fonts/Roboto.ttf"
        },
        {
          "Roboto-Bold": "./assets/fonts/Roboto-Bold.ttf"
        },
        {
          "Roboto-Light": "./assets/fonts/Roboto-Light.ttf"
        },
        {
          "Roboto-Medium": "./assets/fonts/Roboto-Medium.ttf"
        },
        {
          "Roboto-Thin": "./assets/fonts/Roboto-Thin.ttf"
        }
      ]
    }]
  ]
}
